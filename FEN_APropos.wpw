  procedures :
   -
     name : Redimensionne
     procedure_id : 1466844435253474241
     type_code : 14
     code : |1+
      // Résumé : Adapte la hauteur de la fenêtre en fonction de la taille du texte du libellé long
      // Syntaxe :
      // Redimensionne ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE Redimensionne()
      
      nHauteurTexte est un entier = CoordonnéeEditeurVersEcran(TexteHauteur(LIB_APropos,LIB_APropos))
      SI (nHauteurTexte + LIB_APropos..Y) > MaFenêtre..Hauteur ALORS
      	// le texte dépasse
      	LIB_APropos..Hauteur = nHauteurTexte
      	
      	MaFenêtre..Hauteur = nHauteurTexte + LIB_APropos..Y + CoordonnéeEditeurVersEcran(16)
      FIN
     type : 458752
   -
     name : FenMobileAdapteTaille
     procedure_id : 1474334299877867639
     type_code : 14
     code : |1+
      // Résumé : Adapte la taille de la fenêtre au contenu du champ texte
      // Syntaxe :
      //FenMobileAdapteTaille (<sNomChamp> est chaîne)
      //
      // Paramètres :
      //	sNomChamp (chaîne UNICODE) : Nom du champ contenant le texte long
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE FenMobileAdapteTaille(sNomChamp est une chaine)
      
      nMargeBas est un entier = CoordonnéeEditeurVersEcran(10)
      nMargeSecurité est un entier = CoordonnéeEditeurVersEcran(20)
      
      si EnModeiOS() ALORS
      	nMargeSecurité = CoordonnéeEditeurVersEcran(40)
      FIN
      
      // Il faut vérifier l'existence du champ
      SI ChampExiste(sNomChamp)=Faux ALORS
      	RETOUR
      FIN
      
      // Redimensionnement du champ
      nHauteurTexte est un entier= TexteHauteur(sNomChamp,{sNomChamp,indChamp}..Libellé,tMultiligne) + nMargeSecurité
      si {sNomChamp,indChamp}..x + nHauteurTexte + nMargeBas < MaFenêtre..HauteurInitiale ALORS
      	nHauteurTexte = MaFenêtre..HauteurInitiale - nMargeBas - {sNomChamp,indChamp}..X
      FIN
      
      {sNomChamp,indChamp}..Hauteur = nHauteurTexte
      
      // Redimensionnement de la fenêtre
      nHauteurFenêtre est un entier
      si {sNomChamp,indChamp}..X + nHauteurTexte + nMargeBas > MaFenêtre..HauteurInitiale ALORS
      	//nHauteurFenêtre = {sNomChamp,indChamp}..X + nHauteurTexte + nMargeBas
      	nHauteurFenêtre = MaFenêtre..HauteurInitiale + (nHauteurTexte - {sNomChamp,indChamp}..HauteurInitiale)
      SINON
      	nHauteurFenêtre = MaFenêtre..HauteurInitiale
      FIN
      
      
      MaFenêtre..HauteurUtile = nHauteurFenêtre
      
     type : 458752
